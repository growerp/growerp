<entity-facade-xml type="seed">
    <!-- owner party type -->

    <moqui.basic.Enumeration description="Owner" enumId="PtyOwner" enumTypeId="PartyType" />

    <moqui.basic.Enumeration enumCode="product_store_id_from_hostname"
        description="Hostname of the html website" enumId="PsstHostname"
        enumTypeId="ProductStoreSettingType" />
    <moqui.basic.Enumeration enumCode="product_store_id_from_secondary_hostname"
        description="Hostname of the secondary backend" enumId="PsstSecBackend"
        enumTypeId="ProductStoreSettingType" />
    <moqui.basic.Enumeration enumCode="product_store_id_measurement_id" description="MeasurementId"
        enumId="PsstMeasurementId" enumTypeId="ProductStoreSettingType" />

    <!-- remove required permission to approved invoice used in import -->
    <moqui.basic.StatusFlowTransition statusFlowId="InvoicePayable" statusId="InvoiceIncoming"
        toStatusId="InvoiceApproved" transitionSequence="2" transitionName="Approve"
        userPermissionId="" />
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="InvoiceReceived"
        toStatusId="InvoiceApproved" transitionName="Approve" userPermissionId="" />
    <!-- quick complete payment -->
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="PmntAuthorized"
        toStatusId="PmntConfirmed" transitionName="Confirm" />


    <!-- Configuration send email GMAIL-->
    <!--moqui.basic.email.EmailServer
    emailServerId="SYSTEM"
            smtpHost="smtp.gmail.com" smtpPort="587" smtpStartTls="Y" smtpSsl="N"
            storeHost="imap.gmail.com" storePort="993" storeProtocol="imaps" storeFolder=""
            storeDelete="N" storeMarkSeen="Y" storeSkipSeen="Y" mailUsername="${SMTP_USER}"
    mailPassword="${SMTP_PASSWORD}"/-->
    <!-- email opensrs -->
    <moqui.basic.email.EmailServer emailServerId="SYSTEM"
        smtpHost="smtp.growerp.com.cust.hostedemail.com" smtpPort="465" smtpStartTls="Y" smtpSsl="Y"
        storeHost="mail.growerp.com.cust.hostedemail.com" storePort="993" storeProtocol="imaps"
        storeFolder="backend"
        storeDelete="N" storeMarkSeen="Y" storeSkipSeen="Y" mailUsername="SMTP_USER"
        mailPassword="SMTP_PASSWORD" />

    <!-- birdsend email registration -->
    <growerp.general.Interface interfaceId="birdSend" baseUrl="https://api.birdsend.co"
        apiVersion="v1"
        value1="BIRDSEND_AUTM_SEQUENCE" apiKey="BIRDSEND_API_KEY" />
    <!-- openmrs -->
    <growerp.general.Interface interfaceId="openmrs" baseUrl="OPENMRS_BASE_URL"
        username="OPENMRS_USERNAME" password="OPENMRS_PASSWORD" />


    <!-- allocate companies to application -->
    <moqui.basic.Enumeration description="Mobile App Name used by company" enumId="PcltMobileApp"
        enumTypeId="PartyClassificationType" />
    <mantle.party.PartyClassification partyClassificationId="AppSupport"
        classificationTypeEnumId="PcltMobileApp"
        description="System support app to management registered companies" />
    <mantle.party.PartyClassification partyClassificationId="AppAdmin"
        classificationTypeEnumId="PcltMobileApp"
        description="Admin app to maintain backoffice data, products, categories, orders" />
    <mantle.party.PartyClassification partyClassificationId="AppRestaurant"
        classificationTypeEnumId="PcltMobileApp" description="App for restaurant,cafe,bar" />
    <mantle.party.PartyClassification partyClassificationId="AppPOS"
        classificationTypeEnumId="PcltMobileApp" description="Point of sale APP" />
    <mantle.party.PartyClassification partyClassificationId="AppEcommerceShop"
        classificationTypeEnumId="PcltMobileApp" description="App for Ecommerce and shop" />
    <mantle.party.PartyClassification partyClassificationId="AppHospital"
        classificationTypeEnumId="PcltMobileApp"
        description="App for Hospital/Clinic/Dentist/Doctor/Beauty Shop" />
    <mantle.party.PartyClassification partyClassificationId="AppService"
        classificationTypeEnumId="PcltMobileApp"
        description="App for Service company, freelancer, wfh" />
    <mantle.party.PartyClassification partyClassificationId="AppHotel"
        classificationTypeEnumId="PcltMobileApp" description="App for Hotel, Bed &amp; Breakfeast" />
    <mantle.party.PartyClassification partyClassificationId="AppRealtor"
        classificationTypeEnumId="PcltMobileApp" description="App for Realtor/property management" />
    <mantle.party.PartyClassification partyClassificationId="AppFreelance"
        classificationTypeEnumId="PcltMobileApp" description="App for a freelancer" />
    <mantle.party.PartyClassification partyClassificationId="AppHealth"
        classificationTypeEnumId="PcltMobileApp" description="Health/hospital/clinic application" />

    <!-- sales opportunity stage -->
    <mantle.sales.opportunity.SalesOpportunityStage opportunityStageId="Prospecting" sequenceNum="1" />
    <mantle.sales.opportunity.SalesOpportunityStage opportunityStageId="Qualification"
        sequenceNum="2" />
    <mantle.sales.opportunity.SalesOpportunityStage opportunityStageId="Demo/Meeting"
        sequenceNum="3" />
    <mantle.sales.opportunity.SalesOpportunityStage opportunityStageId="Proposal" sequenceNum="4" />
    <mantle.sales.opportunity.SalesOpportunityStage opportunityStageId="Quote" sequenceNum="5" />
    <mantle.sales.opportunity.SalesOpportunityStage opportunityStageId="Deleted" sequenceNum="5" />
    <!-- added thumbnail image size for party -->
    <moqui.basic.Enumeration description="Logo Image Thump Nail" enumId="PcntLogoThumbImage"
        enumTypeId="PartyContentType" />
    <moqui.basic.Enumeration description="Face Image Thump Nail" enumId="PcntFaceThumbImage"
        enumTypeId="PartyContentType" />

    <!-- Customer Status -->
    <moqui.basic.StatusType statusTypeId="CustomerStatus" description="Customer Status" />
    <moqui.basic.StatusItem description="Assigned" sequenceNum="1" statusId="CUSTOMER_ASSIGNED"
        statusTypeId="CustomerStatus" />
    <moqui.basic.StatusItem description="Converted" sequenceNum="2" statusId="CUSTOMER_CONVERTED"
        statusTypeId="CustomerStatus" />
    <moqui.basic.StatusItem description="Qualified" sequenceNum="3" statusId="CUSTOMER_QUALIFIED"
        statusTypeId="CustomerStatus" />
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="CUSTOMER_ASSIGNED"
        toStatusId="CUSTOMER_CONVERTED" transitionName="Converted" />
    <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="CUSTOMER_ASSIGNED"
        toStatusId="CUSTOMER_QUALIFIED" transitionName="Qualified" />

    <!-- added images for category -->
    <moqui.basic.Enumeration description="Category Image Thumb Nail" enumId="PcctImageThumbNail"
        enumTypeId="ProductCategoryContentType" />
    <moqui.basic.Enumeration description="Category Image Medium Size" enumId="PcctImageMedium"
        enumTypeId="ProductCategoryContentType" />
    <moqui.basic.Enumeration description="Category Image Large Size" enumId="PcctImageLarge"
        enumTypeId="ProductCategoryContentType" />

    <!-- define a top level category  -->
    <moqui.basic.Enumeration enumId="PctRoot" description="All Products Root category"
        enumTypeId="ProductCategoryType" />
    <!--moqui.basic.Enumeration
    enumId="PctRestaurant" description="Menu for Restauarant" enumTypeId="ProductCategoryType"/-->

    <!-- hotel room reservation new product type: no shipment -->
    <moqui.basic.Enumeration description="Fixed Asset Use" enumId="PtFixedAssetUse"
        enumTypeId="ProductType" />

    <!-- additional todo roles -->
    <mantle.party.RoleType description="Originator" roleTypeId="Originator" />
    <mantle.party.RoleType description="Assignee" roleTypeId="Assignee" />
    <mantle.party.RoleType description="Third Party" roleTypeId="ThirdParty" />


    <!-- ordertaker is also stored in the field enteredByPartyId in orderheader -->
    <mantle.party.RoleType description="Added more after initial entry of the order"
        roleTypeId="OrderTakeMore" /><!--  when entered as an update -->
    <mantle.party.RoleType description="Prepared part of the order" roleTypeId="OrderPrepare" />
    <mantle.party.RoleType description="Served part of the order" roleTypeId="OrderServe" />
    <!-- Cashier used for billing -->

    <!-- order to facilitate a contact person for the 'other' company -->
    <mantle.party.RoleType description="Vendor Contact Person" roleTypeId="VendorContactPerson" />
    <mantle.party.RoleType description="Customer Contact Person" roleTypeId="CustomerContactPerson" />

    <!-- email templates -->
    <!-- EmailTemplate for general notifications -->
    <moqui.basic.email.EmailTemplate emailTemplateId="NOTIFICATION"
        description="Default Notification" emailServerId="SYSTEM"
        emailTypeEnumId="EMT_NOTIFICATION"
        bodyScreenLocation="classpath://screen/NotificationEmail.xml"
        webappName="webroot"
        fromAddress="support@growerp.com" fromName="GrowERP system"
        ccAddresses="" bccAddresses="" subject="${title}" sendPartial="Y" />
    <!-- EmailTemplate for password reset email -->
    <moqui.basic.email.EmailTemplate emailTemplateId="PASSWORD_RESET"
        description="Default Password Reset"
        emailServerId="SYSTEM" emailTypeEnumId="EMT_PWD_RESET"
        bodyScreenLocation="component://growerp/screen/email/PasswordReset.xml"
        fromAddress="support@growerp.com" ccAddresses=""
        bccAddresses="" subject="Password Reset" sendPartial="Y" />
    <!-- welcome email -->
    <moqui.basic.email.EmailTemplate emailTemplateId="WELCOME"
        description="Welcome email with Password"
        emailServerId="SYSTEM" emailTypeEnumId="EMT_SYSTEM"
        bodyScreenLocation="component://growerp/screen/email/Welcome.xml"
        fromAddress="support@growerp.com" fromName="GrowERP system"
        ccAddresses="" bccAddresses="info@growerp.com"
        subject="Welcome and Password!" sendPartial="Y" />
    <!-- export -->
    <moqui.basic.email.EmailTemplate emailTemplateId="GrowerpExportCategories"
        description="GrowERP export Categories"
        emailServerId="SYSTEM" webappName="webroot"
        fromAddress="support@growerp.com" fromName="Support GrowERP" sendPartial="Y"
        subject="Export Categories data"
        bodyScreenLocation="component://growerp/screen/email/CategoriesExportBody.xml">
        <attachments fileName="categories.csv"
            attachmentLocation="component://growerp/screen/email/CategoriesExportAtt.xml"
            screenRenderMode="text" />
    </moqui.basic.email.EmailTemplate>

    <moqui.basic.email.EmailTemplate emailTemplateId="GrowerpExportProducts"
        description="GrowERP export Products"
        emailServerId="SYSTEM" webappName="webroot"
        fromAddress="support@growerp.com" fromName="Support GrowERP" sendPartial="Y"
        subject="Export Products data"
        bodyScreenLocation="component://growerp/screen/email/ProductsExportBody.xml">
        <attachments fileName="products.csv"
            attachmentLocation="component://growerp/screen/email/ProductsExportAtt.xml"
            screenRenderMode="text" />
    </moqui.basic.email.EmailTemplate>

    <moqui.basic.email.EmailTemplate emailTemplateId="GrowerpExportGlAccounts"
        description="GrowERP export GlAccounts"
        emailServerId="SYSTEM" webappName="webroot"
        fromAddress="support@growerp.com" fromName="Support GrowERP" sendPartial="Y"
        subject="Export Ledger GlAccounts data"
        bodyScreenLocation="component://growerp/screen/email/GlAccountsExportBody.xml">
        <attachments fileName="glAccounts.csv"
            attachmentLocation="component://growerp/screen/email/GlAccountsExportAtt.xml"
            screenRenderMode="text" />
    </moqui.basic.email.EmailTemplate>

    <moqui.basic.email.EmailTemplate emailTemplateId="GrowerpExportCompanyUsers"
        description="GrowERP export Companies/Users"
        emailServerId="SYSTEM" webappName="webroot"
        fromAddress="support@growerp.com" fromName="Support GrowERP" sendPartial="Y"
        subject="Export GrowERP Companies/users data"
        bodyScreenLocation="component://growerp/screen/email/CompanyUsersExportBody.xml">
        <attachments fileName="companyUsers.csv"
            attachmentLocation="component://growerp/screen/email/CompanyUsersExportAtt.xml"
            screenRenderMode="text" />
    </moqui.basic.email.EmailTemplate>

    <!--<growerp.InstallServices.deleteAssetClass/>
    existing classes used in accounting -->
    <moqui.basic.Enumeration description="Hotel Room" enumId="AsClsRoom" enumTypeId="AssetClass" />
    <moqui.basic.Enumeration description="Restaurant Table" enumId="AsClsTable"
        enumTypeId="AssetClass" />
    <moqui.basic.Enumeration description="Restaurant Table Area" enumId="AsClsTableArea"
        enumTypeId="AssetClass" />

    <!-- default template Retail Wiki/Content -->
    <moqui.resource.DbResource filename="DEFAULT_ROOT" isFile="N" resourceId="DEFAULT_ROOT"
        parentResourceId="" />
    <moqui.resource.DbResource filename="content.html" isFile="Y" resourceId="default_content"
        parentResourceId="DEFAULT_ROOT">
        <file mimeType="text/html" versionName="01" rootVersionName="01">
            <fileData><![CDATA[<h3>Welcome to Default Retail</h3>]]></fileData>
            <histories versionName="01" versionDate="1485028800000" isDiff="N" />
        </file>
    </moqui.resource.DbResource>
    <!-- this is the directory to put all site content under -->
    <moqui.resource.DbResource filename="content" isFile="N" resourceId="default_content_dir"
        parentResourceId="DEFAULT_ROOT" />
    <!-- default setup for wiki productstore, categories etc -->
    <moqui.resource.wiki.WikiSpace wikiSpaceId="DEFAULT_WS" description="Default Content"
        restrictView="N" allowAnyHtml="Y"
        rootPageLocation="dbresource://DEFAULT_ROOT" decoratorScreenLocation=""
        publicPageUrl="/content/${pagePath}?ver=${versionName}"
        publicAttachmentUrl="/popc/content/attachment/${wikiPageId}/${filename}" />
    <moqui.resource.wiki.WikiPage wikiPageId="default_content" wikiSpaceId="DEFAULT_WS" pagePath="">
        <histories historySeqId="01" versionName="01" changeDateTime="1485028800000" />
    </moqui.resource.wiki.WikiPage>

    <mantle.facility.Facility facilityId="DEFAULT_WH" facilityTypeEnumId="FcTpWarehouse"
        assetAllowIssueOverQoh="Y"
        ownerPartyId="_NA_" facilityName="Default Warehouse">
        <locations locationTypeEnumId="FltPick" description="loc 1" locationSeqId="0001"
            sequenceNum="1" areaId="01" aisleId="01" sectionId="01" levelId="01" positionId="A" />
        <locations locationTypeEnumId="FltPick" description="loc 2" locationSeqId="0002"
            sequenceNum="2" areaId="02" aisleId="01" sectionId="01" levelId="01" positionId="B" />
        <locations locationTypeEnumId="FltPick" description="loc 3" locationSeqId="0003"
            sequenceNum="3" areaId="03" aisleId="01" sectionId="01" levelId="01" positionId="C" />
        <locations locationTypeEnumId="FltPick" description="loc 4" locationSeqId="0004"
            sequenceNum="4" areaId="04" aisleId="01" sectionId="01" levelId="01" positionId="D" />
        <locations locationTypeEnumId="FltPick" description="loc 5" locationSeqId="0005"
            sequenceNum="5" areaId="05" aisleId="01" sectionId="01" levelId="01" positionId="E" />
        <locations locationTypeEnumId="FltPick" description="loc 6" locationSeqId="0006"
            sequenceNum="6" areaId="06" aisleId="01" sectionId="01" levelId="01" positionId="F" />
    </mantle.facility.Facility>

    <!-- Product for eCommerce Search -->
    <org.moqui.impl.EntityServices.cloneDataDocument dataDocumentId="MantleProduct"
        newDataDocumentId="PopcProduct" newIndexName="popc" />
    <moqui.entity.document.DataDocumentCondition dataDocumentId="PopcProduct" conditionSeqId="01"
        fieldNameAlias="productStoreId" fieldValue="POPC_DEFAULT" postQuery="Y" />
    <!-- NOTE: popc ecommerce search screen creates own links like /popc/Product/Detail/${productId} -->
    <moqui.entity.feed.DataFeed dataFeedId="PopCommerceSearch" dataFeedTypeEnumId="DTFDTP_RT_PUSH"
        feedName="PopCommerce Search"
        feedReceiveServiceName="org.moqui.search.SearchServices.index#DataDocuments"
        feedDeleteServiceName="org.moqui.search.SearchServices.delete#DataDocument">
        <documents dataDocumentId="PopcProduct" />
    </moqui.entity.feed.DataFeed>

    <!-- run index for search -->
    <!--org.moqui.search.SearchServices.indexDataFeedDocuments
    dataFeedId="MantleSearch" />
    <org.moqui.search.SearchServices.indexDataFeedDocuments dataFeedId="PopCommerceSearch" /-->


    <mantle.product.store.ProductStore productStoreId="POPC_DEFAULT" storeName="Default Store"
        organizationPartyId="_NA_" inventoryFacilityId="DEFAULT_WH"
        reservationOrderEnumId="AsResOrdFifoRec"
        wikiSpaceId="DEFAULT_WS" defaultLocale="en_US" defaultCurrencyUomId="USD"
        taxGatewayConfigId="LOCAL"
        requireInventory="Y" productDataDocumentId="PopcProduct">

        <shipOptions carrierPartyId="_NA_" shipmentMethodEnumId="ShMthGround" sequenceNum="1" />
        <shipOptions carrierPartyId="_NA_" shipmentMethodEnumId="ShMthNextDay" sequenceNum="2" />
        <shipOptions carrierPartyId="_NA_" shipmentMethodEnumId="ShMthPickUp" sequenceNum="3" />
        <shippingGateways carrierPartyId="_NA_" shippingGatewayConfigId="NA_LOCAL" />
        <paymentGateways paymentInstrumentEnumId="PiCreditCard"
            paymentGatewayConfigId="TEST_APPROVE" />
    </mantle.product.store.ProductStore>

    <moqui.basic.Enumeration description="Stripe Gateway" enumId="PgtStripe"
        enumTypeId="PaymentGatewayType" />
    <mantle.account.method.PaymentGatewayConfig paymentGatewayConfigId="STRIPE"
        paymentGatewayTypeEnumId="PgtStripe" description="Stripe Production"
        authorizeServiceName="Stripe.StripePaymentServices.authorize#Payment"
        captureServiceName="Stripe.StripePaymentServices.capture#Payment"
        releaseServiceName="Stripe.StripePaymentServices.release#Payment"
        refundServiceName="Stripe.StripePaymentServices.refund#Payment" />

    <!-- ecommerce and general root categories  -->
    <mantle.product.category.ProductCategory productCategoryId="PopcAllProducts" ownerPartyId="_NA_"
        productCategoryTypeEnumId="PctRoot" categoryName="All Products Category" /><!-- general root
    category -->
    <mantle.product.store.ProductStoreCategory productStoreId="POPC_DEFAULT"
        productCategoryId="PopcAllProducts"
        storeCategoryTypeEnumId="PsctSearch" fromDate="1265184000000" thruDate="" />
    <mantle.product.store.ProductStoreCategory productStoreId="POPC_DEFAULT"
        productCategoryId="PopcAllProducts"
        storeCategoryTypeEnumId="PsctPurchaseAllow" fromDate="1265184000000" thruDate="" />
    <mantle.product.store.ProductStoreCategory productStoreId="POPC_DEFAULT"
        productCategoryId="PopcAllProducts"
        storeCategoryTypeEnumId="PsctViewAllow" fromDate="1265184000000" thruDate="" />

    <mantle.product.category.ProductCategory productCategoryId="PopcDeals" ownerPartyId="_NA_"
        productCategoryTypeEnumId="PctCatalog" categoryName="Deals" />
    <mantle.product.store.ProductStoreCategory productStoreId="POPC_DEFAULT"
        productCategoryId="PopcDeals"
        storeCategoryTypeEnumId="PsctPromotions" fromDate="1265184000000" thruDate="" />

    <mantle.product.category.ProductCategory productCategoryId="PopcNew" ownerPartyId="_NA_"
        productCategoryTypeEnumId="PctCatalog" categoryName="New"
        description="Used on the E-Commerce website, products in this category will appear in the 'new' section" />
    <mantle.product.store.ProductStoreCategory productStoreId="POPC_DEFAULT"
        productCategoryId="PopcNew"
        storeCategoryTypeEnumId="PsctNewProducts" fromDate="1265184000000" thruDate="" />

    <mantle.product.category.ProductCategory productCategoryId="PopcFeatured" ownerPartyId="_NA_"
        productCategoryTypeEnumId="PctCatalog" categoryName="Featured"
        description="Used on the E-Commerce website, products in this category will appear in the 'featured' section" />
    <mantle.product.store.ProductStoreCategory productStoreId="POPC_DEFAULT"
        productCategoryId="PopcFeatured"
        storeCategoryTypeEnumId="PsctFeatured" fromDate="1265184000000" thruDate="" />

    <mantle.product.category.ProductCategory productCategoryId="PopcHome" ownerPartyId="_NA_"
        productCategoryTypeEnumId="PctCatalog" categoryName="Home"
        description="Used on the E-Commerce website, products in this category will appear in the 'home' section" />
    <mantle.product.store.ProductStoreCategory productStoreId="POPC_DEFAULT"
        productCategoryId="PopcHome"
        storeCategoryTypeEnumId="PsctHomePage" fromDate="1265184000000" thruDate="" />

    <mantle.product.category.ProductCategory productCategoryId="PopcBrowseRoot" ownerPartyId="_NA_"
        productCategoryTypeEnumId="PctCatalog" categoryName="Root"
        description="Used on the E-Commerce website, products in this category will appear in the 'root' section" />
    <mantle.product.store.ProductStoreCategory productStoreId="POPC_DEFAULT"
        productCategoryId="PopcBrowseRoot"
        storeCategoryTypeEnumId="PsctBrowseRoot" fromDate="1265184000000" thruDate="" />


    <!-- product data -->
    <mantle.product.feature.ProductFeature productFeatureId="SizeSmall"
        productFeatureTypeEnumId="PftSize" description="Small" abbrev="SM" />
    <mantle.product.feature.ProductFeature productFeatureId="SizeMedium"
        productFeatureTypeEnumId="PftSize" description="Medium" abbrev="MD" />
    <mantle.product.feature.ProductFeature productFeatureId="SizeLarge"
        productFeatureTypeEnumId="PftSize" description="Large" abbrev="LG" />
    <mantle.product.feature.ProductFeature productFeatureId="SizeXL"
        productFeatureTypeEnumId="PftSize" description="Extra Large" abbrev="XL" />

    <mantle.product.feature.ProductFeature productFeatureId="ColorBlue"
        productFeatureTypeEnumId="PftColor" description="Blue" abbrev="BU" />
    <mantle.product.feature.ProductFeature productFeatureId="ColorBlack"
        productFeatureTypeEnumId="PftColor" description="Black" abbrev="BL" />
    <mantle.product.feature.ProductFeature productFeatureId="ColorRed"
        productFeatureTypeEnumId="PftColor" description="Red" abbrev="RD" />
    <mantle.product.feature.ProductFeature productFeatureId="ColorGreen"
        productFeatureTypeEnumId="PftColor" description="Green" abbrev="GR" />
    <mantle.product.feature.ProductFeature productFeatureId="ColorYellow"
        productFeatureTypeEnumId="PftColor" description="Yellow" abbrev="YL" />
    <mantle.product.feature.ProductFeature productFeatureId="ColorWhite"
        productFeatureTypeEnumId="PftColor" description="White" abbrev="WH" />

    <!-- poprest store data -->
    <moqui.basic.Enumeration enumCode="product_store_id_from_hostname"
        description="Product Store ID From Hostname" enumId="PsstHostname"
        enumTypeId="ProductStoreSettingType" />
    <moqui.basic.Enumeration enumCode="template_server_content"
        description="Template Server Content" enumId="PsstContentLocation"
        enumTypeId="ProductStoreSettingType" />

    <dbResources resourceId="GROWERP_LOGO" filename="growerp_logo.jpg" isFile="Y"
        parentResourceId="growerp_images" />
    <moqui.resource.DbResourceFile resourceId="GROWERP_LOGO" rootVersionName="01"
        mimeType="image/jpeg" versionName="01">
        <fileData><![CDATA[]]></fileData>
    </moqui.resource.DbResourceFile>

</entity-facade-xml>