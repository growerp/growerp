        <member-entity entity-alias="PROD" entity-name="mantle.product.Product"
            join-from-alias="OITM" join-optional="true">
            <key-map field-name="productId" />
        </member-entity>
        <member-entity entity-alias="ASIC"
            entity-name="mantle.product.issuance.AssetAndIssuanceCost" join-from-alias="OITM"
            join-optional="true" sub-select="true"><!-- sub-select on view-entity for join in
            sub-select -->
            <key-map field-name="orderId" />
