<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">
    <!-- ========================================================= -->
    <!--  - mantle.order -->
    <!--  - mantle.order.return -->
    <!-- ========================================================= -->


    <!-- ========================================================= -->
    <!-- mantle.order -->
    <!-- ========================================================= -->

    <view-entity entity-name="OrderHeaderAndPart" package="mantle.order">
        <member-entity entity-alias="ORHD" entity-name="mantle.order.OrderHeader" />
        <member-entity entity-alias="OPRT" entity-name="mantle.order.OrderPart"
            join-from-alias="ORHD">
            <key-map field-name="orderId" />
        </member-entity>
        <alias-all entity-alias="ORHD" />
        <alias-all entity-alias="OPRT">
            <exclude field="statusId" />
        </alias-all>
        <alias name="partStatusId" entity-alias="OPRT" field="statusId" />
        <!-- the lastUpdatedStamp on OrderHeader gets all major order updates because of the
        revisionNumber increment on major field changes -->
        <alias name="lastUpdatedStamp" entity-alias="ORHD" />
    </view-entity>

    <view-entity entity-name="OrderPartFindView" package="mantle.order">
        <!-- NOTE: this is one of various entities with a LOT of aliased fields, only select those
        that are needed! -->
        <member-entity entity-alias="OPRT" entity-name="mantle.order.OrderPart" />
        <member-entity entity-alias="ORHD" entity-name="mantle.order.OrderHeader"
            join-from-alias="OPRT">
            <key-map field-name="orderId" />
        </member-entity>
        <!-- for finding order parts by productId and perhaps other fields, generally filter by
        these only (don't select) -->
        <member-entity entity-alias="OITM" entity-name="mantle.order.OrderItem"
            join-from-alias="OPRT" join-optional="true">
            <key-map field-name="orderId" />
            <key-map field-name="orderPartSeqId" />
        </member-entity>

        <member-entity entity-alias="CSTRL" entity-name="mantle.party.PartyRole"
            join-from-alias="OPRT" join-optional="true">
            <key-map field-name="customerPartyId" related="partyId" />
        </member-entity>
        <member-entity entity-alias="CSCLS" entity-name="mantle.party.PartyClassificationAppl"
            join-from-alias="OPRT" join-optional="true">
            <key-map field-name="customerPartyId" related="partyId" />
        </member-entity>
        <member-entity entity-alias="VNDRL" entity-name="mantle.party.PartyRole"
            join-from-alias="OPRT" join-optional="true">
            <key-map field-name="vendorPartyId" related="partyId" />
        </member-entity>

        <member-relationship entity-alias="OPPTY" join-from-alias="OPRT" relationship="parties"
            join-optional="true" />
        <member-relationship entity-alias="SMEN" join-from-alias="OPRT"
            relationship="shipmentMethod" join-optional="true" />

        <member-entity entity-alias="SHCBIS" entity-name="mantle.order.ShipmentCountByItemSource"
            join-from-alias="OITM"
            join-optional="true" sub-select="true">
            <key-map field-name="orderId" />
            <key-map field-name="orderItemSeqId" />
        </member-entity>

        <!-- for finding order parts by quantity reserved or reserved in a particular location type
        (like locationTypeEnumId=FltPick) -->
        <member-entity entity-alias="AARS"
            entity-name="mantle.product.issuance.AssetAndReservationSummary" join-from-alias="OITM"
            sub-select="true" join-optional="true">
            <key-map field-name="orderId" />
            <key-map field-name="orderItemSeqId" />
        </member-entity>
        <member-entity entity-alias="AAIS"
            entity-name="mantle.product.issuance.AssetIssuanceSummary" join-from-alias="OITM"
            sub-select="true" join-optional="true">
            <key-map field-name="orderId" />
            <key-map field-name="orderItemSeqId" />
        </member-entity>
        <!-- for orders with PtPickAssembly type products also need BOM total quantity -->
        <member-entity entity-alias="PROD" entity-name="mantle.product.Product"
            join-from-alias="OITM" join-optional="true">
            <key-map field-name="productId" />
        </member-entity>
        <!-- NOTE: join PAS from PROD instead of OITM so that PROD will be included if any PAS
        fields selected (ie play the autotrim) -->
        <member-entity entity-alias="PAS" entity-name="mantle.product.ProductAssocSummary"
            join-from-alias="PROD"
            sub-select="true" join-optional="true">
            <key-map field-name="productId" />
        </member-entity>

        <member-relationship entity-alias="PADR" join-from-alias="OPRT" relationship="postalAddress" />

        <alias-all entity-alias="ORHD" />
        <alias-all entity-alias="OPRT">
            <exclude field="statusId" />
        </alias-all>
        <alias name="partStatusId" entity-alias="OPRT" field="statusId" />
        <alias name="itemTypeEnumId" entity-alias="OITM" />
        <alias name="productId" entity-alias="OITM" />
        <alias name="quantity" entity-alias="OITM" />
        <alias name="quantityTotal" entity-alias="OITM" field="quantity" function="sum" />

        <alias name="customerRoleTypeId" entity-alias="CSTRL" field="roleTypeId" />
        <alias name="vendorRoleTypeId" entity-alias="VNDRL" field="roleTypeId" />
        <alias name="customerClassificationId" entity-alias="CSCLS" field="partyClassificationId" />
        <alias name="customerClassFromDate" entity-alias="CSCLS" field="fromDate" />
        <alias name="customerClassThruDate" entity-alias="CSCLS" field="thruDate" />
        <alias name="otherPartyId" entity-alias="OPPTY" field="partyId" />
        <alias name="otherRoleTypeId" entity-alias="OPPTY" field="roleTypeId" />

        <alias name="shipmentMethodSequenceNum" entity-alias="SMEN" field="sequenceNum" />

        <alias name="shipItemQuantity" entity-alias="SHCBIS" field="quantity" />
        <alias name="shipWorkEffortId" entity-alias="SHCBIS" />
        <alias name="shipmentCount" entity-alias="SHCBIS" />

        <alias name="locationTypeEnumId" entity-alias="AARS" />
        <alias name="resQuantity" entity-alias="AARS" field="quantity" function="sum" />
        <alias name="quantityNotAvailable" entity-alias="AARS" function="sum" />
        <alias name="issuedQuantity" entity-alias="AAIS" field="quantity" function="sum" />
        <alias name="itemBomQuantityTotal" function="sum" entity-alias="PAS"
            field="bomQuantityTotal" type="number-decimal">
            <case>
                <when expression="PROD.PRODUCT_TYPE_ENUM_ID = 'PtPickAssembly'">
                    <complex-alias operator="*">
                        <complex-alias-field entity-alias="OITM" field="quantity" />
                        <complex-alias-field entity-alias="PAS" field="bomQuantityTotal"
                            default-value="1" />
                    </complex-alias>
                </when>
                <else>
                    <complex-alias expression="OITM.QUANTITY" />
                </else>
            </case>
        </alias>

        <alias name="stateProvinceGeoId" entity-alias="PADR" />
        <alias name="countryGeoId" entity-alias="PADR" />
    </view-entity>
    <view-entity entity-name="ShipmentCountByItemSource" package="mantle.order">
        <member-entity entity-alias="SHIS" entity-name="mantle.shipment.ShipmentItemSource" />
        <member-entity entity-alias="SHMT" entity-name="mantle.shipment.Shipment"
            join-from-alias="SHIS">
            <key-map field-name="shipmentId" />
        </member-entity>
        <alias name="orderId" entity-alias="SHIS" />
        <alias name="orderItemSeqId" entity-alias="SHIS" />
        <alias name="quantity" entity-alias="SHIS" />
        <alias name="shipWorkEffortId" entity-alias="SHMT" />
        <alias name="shipmentCount" entity-alias="SHMT" field="shipmentId" function="count" />
    </view-entity>

    <view-entity entity-name="OrderHeaderAndPartSummary" package="mantle.order">
        <member-entity entity-alias="ORHD" entity-name="mantle.order.OrderHeader" />
        <member-entity entity-alias="OPRT" entity-name="mantle.order.OrderPart"
            join-from-alias="ORHD">
            <key-map field-name="orderId" />
        </member-entity>
        <alias-all entity-alias="ORHD" />
        <alias-all entity-alias="OPRT">
            <exclude field="statusId" />
            <exclude field="partTotal" />
        </alias-all>
        <alias name="partStatusId" entity-alias="OPRT" field="statusId" />
        <alias name="orderCount" entity-alias="ORHD" field="orderId" function="count-distinct" />
        <alias name="partTotal" entity-alias="OPRT" function="sum" />
    </view-entity>
    <view-entity entity-name="OrderHeaderPartFacilityStatus" package="mantle.order">
        <!-- NOTE: this is one of various entities with a LOT of aliased fields, only select those
        that are needed! -->
        <member-entity entity-alias="ORHD" entity-name="mantle.order.OrderHeader" />
        <member-entity entity-alias="OPRT" entity-name="mantle.order.OrderPart"
            join-from-alias="ORHD">
            <key-map field-name="orderId" />
        </member-entity>
        <member-entity entity-alias="STSI" entity-name="moqui.basic.StatusItem"
            join-from-alias="ORHD">
            <key-map field-name="statusId" />
        </member-entity>
        <member-entity entity-alias="FAC" entity-name="mantle.facility.Facility"
            join-from-alias="OPRT" join-optional="true">
            <key-map field-name="facilityId" />
        </member-entity>
        <member-entity entity-alias="CSTRL" entity-name="mantle.party.PartyRole"
            join-from-alias="OPRT" join-optional="true">
            <key-map field-name="customerPartyId" related="partyId" />
        </member-entity>
        <member-entity entity-alias="CSCLS" entity-name="mantle.party.PartyClassificationAppl"
            join-from-alias="OPRT" join-optional="true">
            <key-map field-name="customerPartyId" related="partyId" />
        </member-entity>
        <member-entity entity-alias="VNDRL" entity-name="mantle.party.PartyRole"
            join-from-alias="OPRT" join-optional="true">
            <key-map field-name="vendorPartyId" related="partyId" />
        </member-entity>
        <alias-all entity-alias="ORHD">
            <exclude field="pseudoId" />
        </alias-all>
        <alias-all entity-alias="OPRT">
            <exclude field="statusId" />
        </alias-all>
        <alias name="orderPseudoId" entity-alias="ORHD" field="pseudoId" />
        <alias name="partStatusId" entity-alias="OPRT" field="statusId" />
        <alias name="statusDescription" entity-alias="STSI" field="description" />
        <alias name="facilityName" entity-alias="FAC" />
        <alias name="pseudoId" entity-alias="FAC" />
        <alias name="customerRoleTypeId" entity-alias="CSTRL" field="roleTypeId" />
        <alias name="vendorRoleTypeId" entity-alias="VNDRL" field="roleTypeId" />
        <alias name="customerClassificationId" entity-alias="CSCLS" field="partyClassificationId" />
        <alias name="customerClassFromDate" entity-alias="CSCLS" field="fromDate" />
        <alias name="customerClassThruDate" entity-alias="CSCLS" field="thruDate" />
    </view-entity>
    <view-entity entity-name="OrderPartPartyDetail" package="mantle.order">
        <member-entity entity-alias="OPP" entity-name="mantle.order.OrderPartParty" />
        <member-entity entity-alias="RTP" entity-name="mantle.party.RoleType" join-from-alias="OPP">
            <key-map field-name="roleTypeId" />
        </member-entity>
        <member-entity entity-alias="PTY" entity-name="mantle.party.Party" join-from-alias="OPP">
            <key-map field-name="partyId" />
        </member-entity>
        <member-entity entity-alias="PER" entity-name="mantle.party.Person" join-from-alias="PTY"
            join-optional="true">
            <key-map field-name="partyId" />
        </member-entity>
        <member-entity entity-alias="ORG" entity-name="mantle.party.Organization"
            join-from-alias="PTY" join-optional="true">
            <key-map field-name="partyId" />
        </member-entity>
        <alias-all entity-alias="OPP">
            <exclude field="partyId" />
        </alias-all>
        <alias-all entity-alias="RTP" />
        <alias-all entity-alias="PTY" />
        <alias-all entity-alias="PER" />
        <alias-all entity-alias="ORG" />
    </view-entity>
    <view-entity entity-name="OrderItemDetail" package="mantle.order">
        <!-- NOTE: this is one of various entities with a LOT of aliased fields, only select those
        that are needed! -->
        <member-entity entity-alias="OITM" entity-name="mantle.order.OrderItem" />
        <member-entity entity-alias="OHDR" entity-name="mantle.order.OrderHeader"
            join-from-alias="OITM">
            <key-map field-name="orderId" />
        </member-entity>
        <member-entity entity-alias="OPRT" entity-name="mantle.order.OrderPart"
            join-from-alias="OITM">
            <key-map field-name="orderId" />
            <key-map field-name="orderPartSeqId" />
        </member-entity>
        <member-entity entity-alias="CSTRL" entity-name="mantle.party.PartyRole"
            join-from-alias="OPRT" join-optional="true">
            <key-map field-name="customerPartyId" related="partyId" />
        </member-entity>
        <member-entity entity-alias="VNDRL" entity-name="mantle.party.PartyRole"
            join-from-alias="OPRT" join-optional="true">
            <key-map field-name="vendorPartyId" related="partyId" />
        </member-entity>
        <alias-all entity-alias="OITM">
            <exclude field="priority" />
        </alias-all>
        <alias-all entity-alias="OPRT">
            <exclude field="statusId" />
        </alias-all>
        <alias-all entity-alias="OHDR" />
        <alias name="partStatusId" entity-alias="OPRT" field="statusId" />
        <alias name="customerRoleTypeId" entity-alias="CSTRL" field="roleTypeId" />
        <alias name="vendorRoleTypeId" entity-alias="VNDRL" field="roleTypeId" />
    </view-entity>
    <view-entity entity-name="OrderItemProductDetail" package="mantle.order">
        <!-- NOTE: this is one of various entities with a LOT of aliased fields, only select those
        that are needed! -->
        <member-entity entity-alias="OITM" entity-name="mantle.order.OrderItem" />
        <member-entity entity-alias="OHDR" entity-name="mantle.order.OrderHeader"
            join-from-alias="OITM">
            <key-map field-name="orderId" />
        </member-entity>
        <member-entity entity-alias="OPRT" entity-name="mantle.order.OrderPart"
            join-from-alias="OITM">
            <key-map field-name="orderId" />
            <key-map field-name="orderPartSeqId" />
        </member-entity>
        <member-entity entity-alias="CSTRL" entity-name="mantle.party.PartyRole"
            join-from-alias="OPRT" join-optional="true">
            <key-map field-name="customerPartyId" related="partyId" />
        </member-entity>
        <member-entity entity-alias="VNDRL" entity-name="mantle.party.PartyRole"
            join-from-alias="OPRT" join-optional="true">
            <key-map field-name="vendorPartyId" related="partyId" />
        </member-entity>
        <member-entity entity-alias="PROD" entity-name="mantle.product.Product"
            join-from-alias="OITM">
            <key-map field-name="productId" />
        </member-entity>
        <member-entity entity-alias="PCM"
            entity-name="mantle.product.category.ProductCategoryMember" join-from-alias="OITM"
            join-optional="true">
            <key-map field-name="productId" />
        </member-entity>
        <member-entity entity-alias="PFA1" entity-name="mantle.product.feature.ProductFeatureAppl"
            join-from-alias="OITM" join-optional="true">
            <key-map field-name="productId" />
        </member-entity>
        <member-entity entity-alias="PFA2" entity-name="mantle.product.feature.ProductFeatureAppl"
            join-from-alias="OITM" join-optional="true">
            <key-map field-name="productId" />
        </member-entity>
        <member-entity entity-alias="PFA3" entity-name="mantle.product.feature.ProductFeatureAppl"
            join-from-alias="OITM" join-optional="true">
            <key-map field-name="productId" />
        </member-entity>
        <alias-all entity-alias="OITM">
            <exclude field="productCategoryId" />
            <exclude field="quantity" />
            <exclude field="unitAmount" />
            <exclude field="priority" />
        </alias-all>
        <alias-all entity-alias="OPRT">
            <exclude field="statusId" />
            <exclude field="signatureRequiredEnumId" />
        </alias-all>
        <alias-all entity-alias="OHDR">
            <exclude field="pseudoId" />
        </alias-all>
        <alias-all entity-alias="PROD">
            <exclude field="statusId" />
            <exclude field="comments" />
        </alias-all>
        <alias name="orderPseudoId" entity-alias="OHDR" field="pseudoId" />
        <alias name="partStatusId" entity-alias="OPRT" field="statusId" />
        <alias name="customerRoleTypeId" entity-alias="CSTRL" field="roleTypeId" />
        <alias name="vendorRoleTypeId" entity-alias="VNDRL" field="roleTypeId" />
        <alias name="productCategoryId" entity-alias="PCM" />
        <alias name="totalAmount" function="sum">
            <complex-alias operator="*">
                <complex-alias-field entity-alias="OITM" field="quantity" />
                <complex-alias-field entity-alias="OITM" field="unitAmount" />
            </complex-alias>
        </alias>
        <alias entity-alias="OITM" name="quantity" function="sum" />
        <alias entity-alias="PFA1" name="productFeatureId1" field="productFeatureId" />
        <alias entity-alias="PFA2" name="productFeatureId2" field="productFeatureId" />
        <alias entity-alias="PFA3" name="productFeatureId3" field="productFeatureId" />
    </view-entity>
    <view-entity entity-name="OrderItemAndPart" package="mantle.order">
        <!-- NOTE: this is one of various entities with a LOT of aliased fields, only select those
        that are needed! -->
        <member-entity entity-alias="OITM" entity-name="mantle.order.OrderItem" />
        <member-entity entity-alias="OPRT" entity-name="mantle.order.OrderPart"
